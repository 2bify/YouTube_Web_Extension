{
    "manifest_version": 3,
    "name": "2bify",
    "version": "1.0",
    "icons": {
      "16": "./icons/logo_16.png",
      "32": "./icons/logo_32.png",
      "48": "./icons/logo_48.png",
      "128": "./icons/logo_128.png"
    },
    "description": "A simple extension that predicts positivity of video based on comments",
    "permissions": [
      "activeTab",
      "tabs",
      "storage",
      "declarativeContent",
      "scripting"
    ],
    "host_permissions": ["*://*.youtube.com/*"],
    "background": {
      "service_worker": "background.js"
    },
    "web_accessible_resources": [
      {
        "resources": ["styles.css"],
        "matches": ["*://*.youtube.com/*"]
      }
    ]
}
  